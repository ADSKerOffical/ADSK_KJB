 Callback = function(Value)
togle1 = Value
   while togle1 and wait(0.1) do
for _, player in ipairs(game.Players:GetPlayers()) do
 if player ~= game.Players.LocalPlayer and player.Character.Humanoid.Health > 0 and not table.find(whitelist, player.UserId) then
 for i = 1, 100 do
local args = {
    [1] = "KjM1s-1",
    [2] = workspace:WaitForChild(player.Name),
    [3] = 1009,
    [4] = 100
}

game:GetService("ReplicatedStorage"):WaitForChild("AllRemoteEvents"):WaitForChild("CLientToServerR"):FireServer(unpack(args))
  end
end
end
   end
 end    
})

