Tab:AddButton({
 Name = "Godmode",
 Callback = function()
local rand = game.Players:GetPlayers()[math.random(1, #game.Players:GetPlayers())]
if rand ~= game.Players.LocalPlayer then
 local args = {
    [1] = "ShadowStab",
    [2] = workspace:WaitForChild(rand.Name),
    [3] = "ShadowStabEnemy"
}

game:GetService("ReplicatedStorage"):WaitForChild("AllRemoteEvents"):WaitForChild("CLientToServerR"):FireServer(unpack(args))
 wait(1)
  repeat wait(0.1)
local args = {
    [1] = "SweepKickHit",
    [2] = workspace:WaitForChild(rand.Name)
}

game:GetService("ReplicatedStorage"):WaitForChild("AllRemoteEvents"):WaitForChild("CLientToServerR"):FireServer(unpack(args))
  until rand.Character.Humanoid.Health <= 0
end
   end    
})

