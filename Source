local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Budgie Hub | KJ battlegrounds (ADSKer Version)", HidePremium = true, IntroEnabled = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Control",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

game:GetService("GuiService"):SetGameplayPausedNotificationEnabled(false)
local api = loadstring(game:HttpGet("https://raw.githubusercontent.com/ADSKerOffical/API/refs/heads/main/Api.lua"))()
local whitelist = {4636825706, 5199937747, 6066688097}
setclipboard("https://discord.gg/NVuEjRF6")

local Section = Tab:AddSection({
 Name = "All"
})

Tab:AddButton({
 Name = "Kill all",
 Callback = function()
for _, player in ipairs(game.Players:GetPlayers()) do
 if player ~= game.Players.LocalPlayer and not table.find(whitelist, player.UserId) then
 for i = 1, 100 do
local args = {
    [1] = "KjM1s-1",
    [2] = workspace:WaitForChild(player.Name),
    [3] = 1009,
    [4] = 100
}

game:GetService("ReplicatedStorage"):WaitForChild("AllRemoteEvents"):WaitForChild("CLientToServerR"):FireServer(unpack(args))
  end
end
end
   end    
})

